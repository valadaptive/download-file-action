{
    "name": "download-file-action",
    "description": "Cross-platform way to download and cache a file from a URL.",
    "version": "0.1.0",
    "author": "valadaptive",
    "private": true,
    "homepage": "https://github.com/valadaptive/download-file-action",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/valadaptive/download-file-action.git"
    },
    "bugs": {
        "url": "https://github.com/valadaptive/download-file-action/issues"
    },
    "keywords": [
        "actions",
        "node",
        "download"
    ],
    "exports": {
        ".": "./dist/index.js"
    },
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "bundle": "npm run format:write && npm run package",
        "format:write": "npx prettier --write .",
        "format:check": "npx prettier --check .",
        "lint": "npx eslint . -c ./.github/linters/.eslintrc.yml",
        "package": "npx ncc build src/index.ts -o dist --source-map --license licenses.txt",
        "package:watch": "npm run package -- --watch",
        "all": "npm run format:write && npm run lint && npm run package"
    },
    "license": "MIT",
    "dependencies": {
        "@actions/cache": "^3.2.4",
        "@actions/core": "^1.10.1",
        "@actions/http-client": "^2.2.1",
        "content-disposition": "^0.5.4",
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "@types/content-disposition": "^0.5.8",
        "@types/node": "^20.12.8",
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.8.0",
        "@vercel/ncc": "^0.38.1",
        "eslint": "^8.57.0",
        "eslint-plugin-github": "^4.10.2",
        "eslint-plugin-jsonc": "^2.15.1",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5",
        "prettier-eslint": "^16.3.0",
        "tsx": "^4.9.0",
        "typescript": "^5.4.5"
    },
    "type": "module"
}
